{
  "name": "paper-minis",
  "version": "1.0.0",
  "description": "A tool for choosing creatures to print paper minis for",
  "scripts": {
    "build": "parcel build src/index.html --public-url https://BryceLTaylor.github.io/paper-minis",
    "dev": "parcel src/index.html",
    "format": "prettier --write \"src/**/*.{js,jsx}\" ",
    "format:check": "prettier --check \"src/**/*.{js,jsx}\" ",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "push-gh-pages": "npm run build && gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BryceLTaylor/paper-minis.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BryceLTaylor/paper-minis/issues"
  },
  "homepage": "https://github.com/BryceLTaylor/paper-minis#readme",
  "devDependencies": {
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^4.0.0",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "browserslist": [
    "last 5 Chrome versions",
    "last 5 Firefox versions",
    "last 5 Safari versions",
    "last 5 Edge versions"
  ]
}
